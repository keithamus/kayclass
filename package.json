    "license": "MIT",
    "devDependencies": {
        "karma-cli": "^0.0.4",
        "karma": "^0.12.31",
        "karma-mocha": "^0.1.10",
        "karma-chai": "^0.1.0",
        "karma-sinon": "^1.0.4",
        "karma-sauce-launcher": "^0.2.10",
        "karma-chrome-launcher": "^0.1.7",
        "chai": "^1.9.2",
        "istanbul": "^0.3.7",
        "coveralls": "^2.11.2",
        "sinon": "^1.12.2",
        "sinomocha": "^0.2.3",
        "sinon-chai": "^2.6.0",
        "mocha": "^1.18.0"
    }
